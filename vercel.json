  {
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "cleanDistDir": true
        }
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "http://localhost:5080/api" 
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 200
      }
    ]
  }